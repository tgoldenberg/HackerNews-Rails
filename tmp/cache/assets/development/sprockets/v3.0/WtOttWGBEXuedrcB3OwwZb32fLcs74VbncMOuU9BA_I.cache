        url: '/comments',
        method: 'post',
        dataType: 'json',
        data: target.serialize()
      }).done(function(data) {
        return console.log(data);
      }).fail(function(error) {
        return console.log(error);
      });
      return false;
    });
    return $('.open-nested-form').on('click', function(e) {
      var target;
      e.preventDefault;
      target = $(e.target);
      if (target.next().hasClass('hidden')) {
        target.next().removeClass('hidden');
        return false;
      } else {
        target.next().addClass('hidden');
        return false;
      }
    });
  });

}).call(this);
