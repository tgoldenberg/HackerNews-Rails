<br/><br/>
<p><b>Create Account</b></p>
<%= form_for @user do |f|  %>
  <%= f.label 'username:', class: 'comhead form-label'%>
  <%= f.text_field :username %><br/>
  <%= f.label :password, value: 'password:', class: 'comhead form-label' %>
  <%= f.password_field :password %><br/>
  <%= f.submit "create account", class: 'simple-submit' %>
<% end %>
